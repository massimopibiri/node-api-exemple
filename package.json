{
  "name": "api-starter",
  "description": "API Starter",
  "author": "",
  "license": "UNLICENSED",
  "version": "1.0.0-dev",
  "repository": {
    "type": "git",
    "url": ""
  },
  "homepage": "",
  "main": "index.js",
  "scripts": {
    "sequelize": "babel-node node_modules/.bin/sequelize $*",
    "build": "babel src -d dist",
    "!build": "tsc",
    "start": "node dist/",
    "dev": "nodemon src --exec babel-node",
    "!dev": "tsc --watch & nodemon dist",
    "dev:test": "yarn db:test && mocha",
    "dev:coverage": "nyc --check-coverage yarn dev:test",
    "lint": "yarn eslint src",
    "!lint": "yarn eslint src --ext ts",
    "pretest": "yarn eslint --ignore-path .gitignore .",
    "test": "mocha dist/**/*.spec.js",
    "!test": "tsc && mocha dist/**/*.spec.js",
    "db:drop": "dropdb --if-exists ${POSTGRES_DB}",
    "db:create": "createdb ${POSTGRES_DB}",
    "db:migrate": "yarn sequelize db:migrate",
    "db:rollback": "yarn sequelize db:migrate:undo",
    "db:seed": "yarn sequelize db:seed:all",
    "db:clear": "yarn db:drop && yarn db:create",
    "db:reset": "yarn db:clear && yarn db:migrate",
    "db:test:create": "createdb ${POSTGRES_DB}_test",
    "db:test:drop": "dropdb --if-exists ${POSTGRES_DB}_test",
    "db:test:clear": "yarn db:test:drop && yarn db:test:create",
    "db:test:migrate": "NODE_ENV=test yarn sequelize db:migrate",
    "db:test:reset": "yarn db:test:clear && yarn db:test:migrate",
    "db:test:seed": "NODE_ENV=test yarn sequelize db:seed:all",
    "db:test": "yarn db:test:reset && yarn db:test:seed",
    "coverage": "nyc --check-coverage --lines 95 --functions 95 --branches 90 yarn test"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.16.0",
    "connect": "^3.6.5",
    "cors": "^2.8.4",
    "entropy-string": "^4.0.3",
    "express": "^4.14.1",
    "express-status-monitor": "^1.0.1",
    "express-validator": "^4.2.1",
    "immutable": "^3.8.2",
    "jsonwebtoken": "^8.1.0",
    "moment": "^2.21.0",
    "morgan": "^1.8.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.15.0",
    "sequelize-cli": "^4.0.0",
    "socket.io": "^2.0.0",
    "socketio-jwt": "^4.5.0",
    "sparkpost": "^2.1.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/node": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/plugin-proposal-decorators": "^7.0.0-beta.51",
    "@babel/polyfill": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "babel-eslint": "^8.2.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-exclude": "^1.0.2",
    "chai-http": "^3.0.0",
    "chai-match": "^1.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsdoc": "^3.5.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "events": "^1.1.1",
    "faker": "^4.1.0",
    "mocha": "^4.0.1",
    "mocha-prepare": "^0.1.0",
    "nock": "^9.0.22",
    "node-mocks-http": "^1.6.6",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "sinon-stub-promise": "^4.0.0",
    "socket.io-client": "^2.0.4"
  },
  "nyc": {
    "exclude": [
      "**/src/db/migrations/**",
      "**/src/db/seeds/**",
      "**/test/**",
      "**/dist/**"
    ]
  }
}
